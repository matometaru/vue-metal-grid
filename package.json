{
  "name": "vue-metal-grid",
  "version": "0.0.3",
  "description": "Grid layout components for Vue.js.",
  "keywords": [
    "grid",
    "vuejs",
    "layout",
    "grid",
    "masonry",
    "responsive"
  ],
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run docs",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --inline --hot",
    "clean": "rimraf lib",
    "build": "babel src/ -d lib/ --ignore \"**/__tests__/*\"",
    "test:lint": "eslint \"src/**/*.js\"",
    "docs": "webpack-dev-server --config docs/webpack.config.js",
    "docs:build": "webpack -p --config docs/webpack.config.production.js",
    "docs:deploy": "npm run docs:build && gh-pages -d docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matometaru/vue-metal-grid.git"
  },
  "author": "matometaru",
  "dependencies": {
    "easy-css-transform-builder": "^0.1.0",
    "element-resize-detector": "^1.1.14",
    "exenv": "^1.2.2",
    "imagesloaded": "^4.1.4",
    "inline-style-prefixer": "^4.0.2",
    "interactjs": "^1.3.3",
    "vue-transition-group-plus": "^0.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-vue": "^2.0.2",
    "cross-env": "^3.0.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-vue": "^5.0.0-beta.1",
    "node-sass": "^4.9.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "vue-hot-reload-api": "^2.3.0",
    "vue-loader": "^11.1.4",
    "vue-router": "^3.0.1",
    "vue-slider-component": "^2.7.5",
    "vue-style-loader": "^4.1.1",
    "vue-template-compiler": "^2.2.6",
    "webpack": "^3.5.6",
    "webpack-dev-server": "^2.7.0"
  },
  "peerDependencies": {
    "vue": "^2.2.6"
  },
  "license": "MIT"
}
