{
  "name": "vue-metal-grid",
  "version": "0.0.4",
  "description": "Grid layout components for Vue.js.",
  "keywords": [
    "grid",
    "vuejs",
    "layout",
    "grid",
    "masonry",
    "responsive"
  ],
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run docs",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --inline --hot",
    "clean": "rimraf lib",
    "build": "babel src/ -d lib/ --ignore \"**/__tests__/*\"",
    "test": "npm run test:lint && npm run test:unit",
    "test:unit": "jest",
    "test:lint": "eslint \"src/**/*.js\" \"docs/js/**/*.js\"",
    "docs": "webpack-dev-server --config docs/webpack.config.js",
    "docs:build": "webpack -p --config docs/webpack.config.production.js",
    "docs:deploy": "npm run docs:build && gh-pages -d docs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/matometaru/vue-metal-grid.git"
  },
  "author": "matometaru",
  "dependencies": {
    "easy-css-transform-builder": "^0.1.0",
    "element-resize-detector": "^1.1.14",
    "exenv": "^1.2.2",
    "imagesloaded": "^4.1.4",
    "inline-style-prefixer": "^4.0.2",
    "interactjs": "^1.3.3",
    "vue": "2.6.8",
    "vue-transition-group-plus": "^0.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/helper-module-imports": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/runtime": "^7.3.4",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.0.0-beta.2",
    "@vue/babel-plugin-transform-vue-jsx": "^1.0.0-beta.2",
    "@vue/babel-preset-jsx": "^1.0.0-beta.2",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.5",
    "cross-env": "^3.0.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.15.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-vue": "^5.0.0-beta.1",
    "jest": "^24.0.0",
    "jest-serializer-vue": "^2.0.2",
    "node-sass": "^4.9.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "sinon": "^7.2.7",
    "vue-hot-reload-api": "^2.3.0",
    "vue-loader": "^14.2.2",
    "vue-router": "^3.0.1",
    "vue-slider-component": "^2.7.5",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.8",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "peerDependencies": {
    "vue": "^2.2.6"
  },
  "license": "MIT"
}
